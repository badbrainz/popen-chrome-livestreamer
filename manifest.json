{
    "name": "Chrome to Native Media Player",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Use a local media player.",
    "icons": {
        "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "contextMenus",
        "notifications",
        "nativeMessaging"
    ]
}
